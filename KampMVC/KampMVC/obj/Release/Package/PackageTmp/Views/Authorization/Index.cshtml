@using EntityLayer.Concrete
@model List<Admin>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Kullanıcı Mail</th>
        <th>Yetki</th>
        <th>Durum</th>
        @*<th>Pasif Yap</th>*@
        <th>Yetkisini Değiştir</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th>@item.AdminID</th>
            <td>@item.AdminUsername</td>
            <td>@item.AdminRole</td>
            <td>Aktif</td>
            @*<td><a href="/AdminCategory//@item.AdminID" class="btn btn-danger">Pasif Yap</a></td>*@
            <td><a href="/Authorization/EditAdmin/@item.AdminID" class="btn btn-warning">Yetkisini Değiştir</a></td>
        </tr>
    }
</table>
<a href="/Authorization/AddAdmin/" class="btn btn-dark">Yeni Admin Ekle</a>
<script src="~/AdminLTE-3.0.4/plugins/jquery/jquery.min.js"></script>